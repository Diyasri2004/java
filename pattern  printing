import java.util.Scanner;

public class MyClass
{
    public static void main(String args[])
    {
        Scanner sobj= new Scanner(System.in);
        int n= sobj.nextInt();
        
        for(int i=1; i<=n; i++)
        {
            for (int j=1; j<=i; j++)
            {
                System.out.print("*");
            }
            System.out.println("");
        }
    }
}
