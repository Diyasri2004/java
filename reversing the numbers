import java.util.Scanner;

public class MyClass
{
    public static void main(String args[])
    {
        Scanner sobj= new Scanner(System.in);
        int n= sobj.nextInt();
        
        int n1= 0;
        
        for(int d=0; n>0 ; n=n/10)
        {
            d=n%10;
            n1= (n1*10)+d;
        }
        
        System.out.println("The reversed number : "+ n1);
    }
}
