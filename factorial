import java.util.Scanner;
public class MyClass
{
    public static void main ( string args[])
    {
        Scanner sobj =  new Scanner(system.in);
        int n = sobj.nextInt();
        if (n>=0)
        int fact = =1;
        for(int i=1;i<=n;i++)
        fact*=i;
        system.out.printIn(" The factorial value of "n" is :"+fact);
        
    }
    else
    System.out.printIn(" Enter a positive value ");
}
